<h2 class="header"> Welcome, Please enter your information into the form</h2>

<!-- ________________________________________________________________________________ -->
<form action="/welcome/register" method="post">
  <!--This is the input box for the user information-->
  <input type="text" name="full_name" placeholder="Enter Name" autofocus/>
  </br>
  <input type="text" name="address" placeholder="Address"/>
  </br>
  <input type="text" name="city" placeholder="City"/>
  </br>
  <input type="text" name="state" placeholder="State"/>
  </br>
  <input type="text" name="zip_code" placeholder="Zip Code"/>
  </br>
  <input type="text" name="country" placeholder="Country"/>
  </br>
  <input type="text" name="email" placeholder="Enter Email"/>
  </br>
<!-- ________________________________________________________________________________ -->
  <!--This is the input box for the phones-->
  <input type="number" name="phone1" placeholder="Enter Phone #1"/>
  <input type="number" name="phone2" placeholder="Enter Phone #2"/>
  <input type="number" name="phone3" placeholder="Enter Phone #3"/>
  </br>
<!-- ________________________________________________________________________________ -->
  <input type="text" name="user_name" placeholder="User Name" required/>
  </br>
  <input type="password" name="password" placeholder="Password" required/>
  </br>
  <hr>
  <input type="submit" value="Submit"/>
</form>
<!-- ________________________________________________________________________________ -->
